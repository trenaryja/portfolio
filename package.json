{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "npx prettier --write **/*.{js,jsx,ts,tsx,css,md,json} --ignore-path .gitignore",
    "lint": "npx eslint --fix **/*.{js,jsx,ts,tsx,css} --ignore-path .gitignore"
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.0",
    "@mui/styles": "^5.12.3",
    "next": "^13.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-tsparticles": "^2.9.3",
    "tsparticles": "^2.9.3",
    "tsparticles-engine": "^2.9.3"
  },
  "devDependencies": {
    "@types/node": "^20.1.3",
    "@types/react": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope ": "off"
    }
  }
}
